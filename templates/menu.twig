{# MENU COMES FROM menu.twig #}
{% if menu %}
    <div style="text-align: center;">
        <ul>
            {% for item in items %}
                <li class="{{ item.classes | join(' ') }}  " style="display: inline-block; white-space: nowrap;">
                    <a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
                    {% include "menu.twig" with {'items': item.children} %}
                </li>
            {% endfor %}
        </ul>
    </div>
{% endif %}


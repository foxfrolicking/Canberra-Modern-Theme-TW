{% block html_head_container %}

	{% include 'html-header.twig' %}
	{% block head %}{% endblock %}

</head>{% endblock %}<body class="body_class" data-template="base.twig">

  </div>
{{ function('wp_body_open') }}
<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>
<header class="header text-slate " >
	{% block header %}
		<div class="wrapper">
			<h1 class="hdr-logo" role="banner">
				<a href="#">CM</a>
			</h1>
			<nav id="nav-main" class="nav-main  py-8" role="navigation">
				{% include "menu.twig" with {'items': menu.get_items} %}
			</nav>
			<!-- #nav -->
		</div>
	{% endblock %}
</header>

<section id="content" role="main" class="content-wrapper text-slate" >
	{% if title %}
		<!-- this title comes from the base.twig file -->
		<h1>{{title}}</h1>
	{% endif %}
	<div class="wrapper">
		{% block content %}
			Sorry, no content
		{% endblock %}
	</div>
	{% if sidebar %}
		<aside class="layout-sidebar">
			{{sidebar}}
		</aside>
	{% endif %}
</section>

{% block footer %}
	<footer id="footer" class="bg-black">
		{% include 'footer.twig' %}
	</footer>
	{{ function('wp_footer') }}
{% endblock %}</body></html>
